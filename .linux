# Add the apps to your path.
export PATH=$PATH:"~/.local/bin"
export PATH=$PATH:"~/Programs"

# Add an alias for xdg-open.
alias open="xdg-open"

# Setup environment variables for linuxbrew.
eval $(/home/linuxbrew/.linuxbrew/bin/brew shellenv)

# An alias for gui emacs.
alias wemacs=/snap/bin/emacs
